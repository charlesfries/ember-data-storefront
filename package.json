{
  "name": "ember-data-storefront",
  "version": "0.18.1",
  "description": "Predictable data-loading APIs for Ember Data",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://embermap.github.io/ember-data-storefront",
  "repository": "https://github.com/embermap/ember-data-storefront",
  "license": "MIT",
